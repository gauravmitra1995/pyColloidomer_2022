for Np in 200;do
     for R in 20.0;do
        for radiusB in 1.0;do
           for Nclusters in 7;do
               for kAB in 200.0;do
                   for dimension in 2;do
                       for koninit in 200.0;do
                             for koffinit in 2.0;do
                               for restlength in 2.0;do
                                      bash clusteranalysis.sh $Np $R $radiusB $Nclusters $kAB $dimension $koninit $koffinit $restlength
                               done
                           done
                       done
                  done
               done
            done
        done
    done
done
exit

